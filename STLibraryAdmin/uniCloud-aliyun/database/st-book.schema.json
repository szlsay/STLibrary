// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": true,
		"create": true,
		"update": true,
		"delete": true
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"isisbn": {
			"bsonType": "bool",
			"description": "是否通过isbn方式",
			"label": "是否通过isbn方式"
		},
		"images": {
			"bsonType": "array",
			"arrayType": "file",
			"fileMediaType": "image",
			"fileExtName": "jpg,png,webp,jpeg",
			"description": "请选择图片",
			"label": "图片"
		},
		"author": {
			"bsonType": "string",
			"description": "请输入作者",
			"label": "作者"
		},
		"bookDesc": {
			"bsonType": "string",
			"description": "请输入内容介绍",
			"label": "内容介绍"
		},
		"bookName": {
			"bsonType": "string",
			"description": "请输入书名",
			"label": "书名"
		},
		"clcCode": {
			"bsonType": "string",
			"description": "请输入类型编码",
			"label": "类型编码"
		},
		"clcName": {
			"bsonType": "string",
			"description": "请输入类型名称",
			"label": "类型名称"
		},
		"isbn": {
			"bsonType": "string",
			"description": "请输入ISBN",
			"label": "ISBN"
		},
		"pictures": {
			"bsonType": "array",
			"arrayType": "string",
			"description": "请输入图片",
			"label": "图片"
		},
		"press": {
			"bsonType": "string",
			"description": "请输入出版社",
			"label": "出版社"
		},
		"pressDate": {
			"bsonType": "string",
			"description": "请输入出版时间",
			"label": "出版时间"
		},
		"pressPlace": {
			"bsonType": "string",
			"description": "请输入出版地址",
			"label": "出版地址"
		}
	}
}